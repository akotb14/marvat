<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Category</title>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
    />
    <link rel="stylesheet" href="../admin/dashboard/assets/css/style.css" />
    <link rel="stylesheet" href="../admin/signup/style.css" />
  </head>
  <body>
    <!-- =============== Navigation ================ -->
    <div class="container">
      <div class="navigation">
        <ul>
          <li>
            <a href="#" class="nav">
              <span class="icon">
                <img src="/logo2.png" alt="" />
              </span>
              <span class="title">The Quen Platform</span>
            </a>
          </li>

          <li>
            <a href="/" class="nav">
              <span class="icon">
                <ion-icon name="home-outline"></ion-icon>
              </span>
              <span class="title">go to home</span>
            </a>
          </li>
          <li>
            <a href="/dashboard" class="nav">
              <span class="icon">
                <ion-icon name="home-outline"></ion-icon>
              </span>
              <span class="title">Dashboard</span>
            </a>
          </li>
          <li>
            <a href="/student" class="nav">
              <span class="icon">
                <ion-icon name="people-outline"></ion-icon>
              </span>
              <span class="title">Students</span>
            </a>
          </li>
          <li>
            <a href="/admin/studentOnline" class="nav">
              <span class="icon">
                <ion-icon name="people-outline"></ion-icon>
              </span>
              <span class="title">studentOnline</span>
            </a>
          </li>
          <li>
            <a href="/lesson" class="nav">
              <span class="icon">
                <ion-icon name="videocam-outline"></ion-icon>
              </span>
              <span class="title">Lessons</span>
            </a>
          </li>
          <li>
            <a href="/admin/homework" class="nav">
              <span class="icon">
                <ion-icon name="newspaper-outline"></ion-icon>
              </span>
              <span class="title">Homework</span>
            </a>
          </li>
          <li>
            <a href="/showStudentsDegree/homework" class="nav">
              <span class="icon">
                <ion-icon name="newspaper-outline"></ion-icon>
              </span>
              <span class="title">show Homework Degree</span>
            </a>
          </li>

          <li>
            <a href="/admin/quiz" class="nav">
              <span class="icon">
                <ion-icon name="clipboard-outline"></ion-icon>
              </span>
              <span class="title">Quiz</span>
            </a>
          </li>
          <li>
            <a href="/showStudentsDegree/quiz" class="nav">
              <span class="icon">
                <ion-icon name="newspaper-outline"></ion-icon>
              </span>
              <span class="title">show Quiz Degree</span>
            </a>
          </li>
          <li>
            <a href="/admin/exam" class="nav">
              <span class="icon">
                <ion-icon name="book-outline"></ion-icon>
              </span>
              <span class="title">Exam</span>
            </a>
          </li>
          <li>
            <a href="/showStudentsDegree/exam" class="nav">
              <span class="icon">
                <ion-icon name="newspaper-outline"></ion-icon>
              </span>
              <span class="title">show Exam Degree</span>
            </a>
          </li>
         
          <li>
            <a href="/sign" class="nav">
              <button class="signout nav">
                <span class="icon">
                  <ion-icon name="log-in-outline"></ion-icon>
                </span>
                <span class="title">Add Student</span>
              </button>
            </a>
          </li>
          
        </ul>
      </div>

      <div class="main">
        <div class="topbar">
          <div class="toggle">
            <ion-icon name="menu-outline"></ion-icon>
          </div>
        </div>
        <div class="container1">
          <div class="admin-Category-form-container">
            <form method="post">
              <input type="hidden" name="_csrf" value="<%=csrfToken%>" />
              <h3>add student</h3>
              <input
                type="text"
                placeholder="fullName"
                name="fullName"
                class="box"
              />
              <% let fullName= validator.find(err => err.instancePath ===
              "/fullName"); %> <% if(fullName){%>
              <h5><%=fullName.message%></h5>
              <%}%>
              <input
                type="text"
                placeholder="cardNumber"
                name="cardNumber"
                class="box"
              />
              <% let cardNumber= validator.find(err => err.instancePath ===
              "/cardNumber"); %> <% if(cardNumber){%>
              <h5><%=cardNumber.message%></h5>
              <%}%> <%if(check && check !=""){%>
              <h5>cardNumber is arleady taken</h5>
              <%}%>
              <input
                type="text"
                placeholder="phoneNumber"
                name="phoneNumber"
                class="box"
              />
             
              
              <select name="educetionlevel">
                <option
                  name="educetionlevel"
                  value=""
                  class="placeholder"
                  disabled
                  selected
                  hidden
                >
                Specialization
                </option>
                <option name="educetionlevel" value="biology">Biology</option>
                <option name="educetionlevel" value="geology">Geology</option>
              </select>
              
            
              <br />
              <br />
             
              <br />
             
              <select name="grade" id="grade">
                <option
                  name="grade"
                  value=""
                  class="placeholder"
                  disabled
                  selected
                  hidden
                >
                  Grade
                </option>
                <option name="grade" value="1st">1st</option>
                <option name="grade" value="2nd">2nd</option>
                <option name="grade" value="3rd">3rd</option>
              </select>

              <br />
              <br />
              <select name="group" id="group">
                <option
                  name="group"
                  value=""
                  class="placeholder"
                  disabled
                  selected
                  hidden
                >
                  Group
                </option>
                <option name="group" value="OnlineStudent">OnlineStudent</option>
                <option name="group" value="Monday">Monday</option>
                <option name="group" value="Tuesday">Tuesday</option>
                <option name="group" value="Wednesday">Wednesday</option>
                <option name="group" value="Thursday">Thursday</option>
                <option name="group" value="Friday">Friday</option>
                <option name="group" value="Saturday">Saturday</option>
                <option name="group" value="Sunday">Sunday</option>
              </select>

              <br />
              <br />
              <br />
              <input
                type="text"
                placeholder="password"
                name="password"
                class="box"
              />
          
             
              <br />
              <br />
              <select name="admin" id="grade">
                <option
                  name="admin"
                  value="false"
                  class="placeholder"
                  disabled
                  selected
                  hidden
                >
                  isAdmin
                </option>
                <option name="admin" value="true">true</option>
                <option name="admin" value="false">false</option>
              </select>

              <br />
              <br />
              <button class="btn">signup</button>
            </form>
          </div>

          
        </div>
      </div>
    </div>

    <script src="../admin/dashboard/assets/js/main.js"></script>
    <script
      type="module"
      src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"
    ></script>
    <script
      nomodule
      src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"
    ></script>
  </body>
</html>
